	"devDependencies": {
		"@biomejs/biome": "2.3.10",
		"@vituum/vite-plugin-pug": "1.1.0",
		"@vituum/vite-plugin-twig": "1.1.0",
		"husky": "9.1.7",
		"lightningcss": "1.30.2",
		"lint-staged": "16.2.7",
		"plop": "4.0.4",
		"sass-embedded": "1.97.1",
		"vite": "7.3.0",
		"vite-plugin-checker": "0.12.0",
		"vite-plugin-static-copy": "3.1.4"
	},
	"engines": {
		"node": ">=22.17.0"
