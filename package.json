{
	"name": "moonkake",
	"description": "Site starter template based on Vite, Scss, Pug/Twig, JS",
	"version": "10.4.0",
	"author": {
		"name": "DevBrains",
		"url": "https://devbrains.io"
	},
	"homepage": "https://github.com/pro-verstka/moonkake#readme",
	"repository": {
		"type": "git",
		"url": "git://github.com/pro-verstka/moonkake.git"
	},
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"tools:format": "biome format ./src --fix",
		"tools:lint": "biome check ./src --fix",
		"tools:generate": "plop",
